{
  "batches": [
    {
      "objects": [
        {% for item in items if item.name not in squareup_item_names %}
        {
          "type": "ITEM",
          "id": "#{{ item.name }}",
          "present_at_all_locations": true,
          "item_data": {
            "name": "{{ item.name }}",
            "description": "{{ item.description }}",
            "variations": [
              {
                "type": "ITEM_VARIATION",
                "id": "#{{ item.name }}_archival",
                "present_at_all_locations": true,
                "item_variation_data": {
                  "item_id": "#{{ item.name }}",
                  "name": "{{ item.type }}",
                  "pricing_type": "FIXED_PRICING",
                  "price_money": {
                    "amount": {{ "%.0f" | format( item.price * 100 ) }},
                    "currency": "USD"
                  }
                }
              }
            ]
          }
        },
        {% endfor %}
      ]
    }
  ],
  "idempotency_key": "{{ 9999999999999999999999 | random | to_uuid }}"
}
